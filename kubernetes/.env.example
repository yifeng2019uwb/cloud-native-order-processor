
# .env.example - Copy to .env and fill in your values

# AWS Configuration
AWS_REGION=us-west-2
AWS_PROFILE=default

# Database Configuration (from Terraform outputs)
DB_PASSWORD=your-secure-password

# Optional: Override Terraform resource names if different
# ORDERS_TABLE_NAME=cloud-native-order-processor-orders
# INVENTORY_TABLE_NAME=cloud-native-order-processor-inventory
# USERS_TABLE_NAME=cloud-native-order-processor-users

# Optional: Override SQS queue names if different
# PAYMENT_QUEUE_NAME=cloud-native-order-processor-payment-processing
# NOTIFICATION_QUEUE_NAME=cloud-native-order-processor-notifications

# Optional: Override SNS topic names if different
# ORDER_NOTIFICATIONS_TOPIC_NAME=cloud-native-order-processor-order-notifications

# Optional: Override Lambda function names if different
# PAYMENT_PROCESSOR_FUNCTION_NAME=cloud-native-order-processor-payment-processor
# NOTIFICATION_SERVICE_FUNCTION_NAME=cloud-native-order-processor-notification-service

# Image tags (optional, defaults to 'latest')
# ORDER_API_IMAGE_TAG=v1.0.0
# INVENTORY_SERVICE_IMAGE_TAG=v1.0.0
# FRONTEND_IMAGE_TAG=v1.0.0
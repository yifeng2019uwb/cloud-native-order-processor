[run]
omit =
    */common/*
    */common/src/*
    */common/tests/*
    */common/setup.py
    */common/build-test.sh
    src/main.py
    src/__init__.py
    */__init__.py
    */config.py
    */tests/*
    */venv/*

[report]
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.:
    raise AssertionError
    raise NotImplementedError